<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-CacheNotFound">
    <DisplayName>RF-CacheNotFound</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json"> { "error" : "data not found in cache"}
        </Payload>
            <StatusCode>404</StatusCode>
            <ReasonPhrase>Not Found</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>