<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-ClientIdentityFilter">
    <DisplayName>RF-ClientIdentityFilter</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Payload contentType="application/json"> {"error" : "either session/AuthHeader is not found."}</Payload>
            <StatusCode>404</StatusCode>
            <ReasonPhrase>ClientIdentity Error</ReasonPhrase>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>