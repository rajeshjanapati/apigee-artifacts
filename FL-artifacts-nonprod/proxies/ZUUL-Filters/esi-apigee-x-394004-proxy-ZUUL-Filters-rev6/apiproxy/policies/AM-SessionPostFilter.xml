<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage continueOnError="false" enabled="true" name="AM-SessionPostFilter">
    <DisplayName>AM-SessionPostFilter</DisplayName>
    <Properties/>
    <Add>
        <Headers>
            <Header name="SessionId">{session.id}</Header>
            <Header name="AccessToken">{jwt-token}</Header>
        </Headers>
    </Add>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>