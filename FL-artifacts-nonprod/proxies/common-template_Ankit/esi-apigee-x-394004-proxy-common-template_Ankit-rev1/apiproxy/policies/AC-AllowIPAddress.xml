<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AccessControl continueOnError="false" enabled="false" name="AC-AllowIPAddress">
    <DisplayName>AC-AllowIPAddress</DisplayName>
    <!-- This is the rule which allows IP address stored in KVM -->
    <IPRules noRuleMatchAction="ALLOW">
        <MatchRule action="ALLOW">
            <SourceAddress mask="{kvm.mask.value}">{kvm.ip.value}</SourceAddress>
        </MatchRule>
    </IPRules>
</AccessControl>