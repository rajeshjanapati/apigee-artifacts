<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Quota continueOnError="false" enabled="true" name="Quota-DailyQuota">
    <DisplayName>Quota-DailyQuota</DisplayName>
    <!-- This policy tells us to maintain a separate quota per application, based on api key,
       and to set the parameters based on the api product that was selected in the 
       VA-VerifyApiKey policy. -->
    <Interval ref="verifyapikey.VA-VerifyApiKey.apiproduct.developer.quota.interval">1</Interval>
    <TimeUnit ref="verifyapikey.VA-VerifyApiKey.apiproduct.developer.quota.timeunit">minute</TimeUnit>
    <Allow countRef="verifyapikey.VA-VerifyApiKey.apiproduct.developer.quota.limit">500</Allow>
    <Identifier ref="verifyapikey.VA-VerifyApiKey.client_id"/>
    <Distributed>true</Distributed>
</Quota>